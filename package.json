{
    "name": "gitbook-plugin-phpclassdisplayer",
    "version": "1.0.2",
    "description": "Display PHP's classes, methods, functions or constants in your GitBook",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/REJack/gitbook-plugin-phpclassdisplayer.git"
    },
    "keywords": [
        "gitbook",
        "php",
        "classdisplayer"
    ],
    "engines": {
        "gitbook": ">=3.0.0-pre.8"
    },
    "gitbook": {
        "properties": {
            "scope": {
                "type": "string",
                "default": "::",
                "description": "Defines the Scope Resolution Operator between Class and Method in anchors."
            },
            "anchors": {
                "type": "bool",
                "default": true,
                "description": "Enables/Disables Anchors."
            }
        }
    },
    "dependencies": {
        "escape-html": "^1.0.3"
    },
    "author": "Raphael Jackstadt <info@rejack.de>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/REJack/gitbook-plugin-phpclassdisplayer/issues"
    },
    "homepage": "https://github.com/REJack/gitbook-plugin-phpclassdisplayer#readme"
}
